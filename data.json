{
  "user": {
    "id": {
      "dataType": "integer",
      "columnName": "id",
      "nullable": false
    },
    "profilePicture": {
      "dataType": "string",
      "columnName": "profile_picture",
      "defaultValue": "/uploads/default.png",
      "nullable": false
    },
    "firstName": {
      "dataType": "string",
      "columnName": "first_name",
      "nullable": false
    },
    "lastName": {
      "dataType": "string",
      "columnName": "last_name",
      "nullable": false
    },
    "login": {
      "dataType": "string",
      "columnName": "login",
      "nullable": false
    },
    "email": {
      "dataType": "string",
      "columnName": "email",
      "nullable": false
    },
    "lang": {
      "dataType": "string",
      "columnName": "lang",
      "defaultValue": "EN",
      "nullable": false
    },
    "password": {
      "dataType": "string",
      "columnName": "password",
      "nullable": false
    }
  },
  "moviesMiniature": {
    "name": {
      "dataType": "string",
      "nullable": false
    },
    "year": {
      "dataType": "integer",
      "nullable": false
    },
    "rating": {
      "dataType": "string",
      "nullable": true,
      "ifNull": "no"
    },
    "coverImage": {
      "dataType": "string",
      "nullable": true,
      "ifNull": "/uploads/coverDefaultImage.png"
    }
  }
}
